{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "dingo/api": "2.0.0-alpha1",
        "encore/laravel-admin": "1.5.*",
        "fideloper/proxy": "~3.3",
        "google/cloud-speech": "^0.9.0",
        "google/cloud-translate": "^1.0",
        "illuminate/html": "^5.0",
        "jadjoubran/laravel-angular": "^0.4.0",
        "laravel-admin-ext/config": "^1.0",
        "laravel-admin-ext/helpers": "^1.0",
        "laravel-admin-ext/media-manager": "^1.0",
        "laravel-admin-ext/scheduling": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "nutsweb/laravel-prerender": "dev-master",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-wechat": "~4.0",
        "overtrue/wechat": "~4.0",
        "owner888/phpspider": "^2.1",
        "pbmedia/laravel-ffmpeg": "^1.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "tymon/jwt-auth": "1.0.0-rc.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },

    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/api/baiduspeech"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },

    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
